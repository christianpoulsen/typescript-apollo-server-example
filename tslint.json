{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [false],
        "member-ordering": [
            false,
            {
              "order": [
                "public-static-field",
                "public-instance-field",
                "public-constructor",
                "private-static-field",
                "private-instance-field",
                "private-constructor",
                "public-instance-method",
                "protected-instance-method",
                "private-instance-method"
              ],
              "alphabetize": false
            }
          ]
    },
    "rulesDirectory": []
}